CREATE TABLE feeds (
    _id INTEGER PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    url TEXT NOT NULL,
    format TEXT NOT NULL,
    siteUrl TEXT NOT NULL,
    iconPath TEXT NOT NULL,
    unreadArticle INTEGER NOT NULL
);

getNumOfRss:
SELECT COUNT(*)
FROM feeds;

getAllFeeds:
SELECT *
FROM feeds
ORDER BY title;

updateTitle:
UPDATE feeds
SET title = ?
WHERE _id = ?;

selectChanges:
SELECT changes();

delete:
DELETE FROM feeds
WHERE _id = ?;
