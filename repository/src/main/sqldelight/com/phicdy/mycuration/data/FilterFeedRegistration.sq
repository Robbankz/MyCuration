CREATE TABLE filterFeedRegistrations (
    _id INTEGER PRIMARY KEY AUTOINCREMENT,
    filterId INTEGER,
    feedId INTEGER,
    FOREIGN KEY(filterId) REFERENCES filters(_id),
    FOREIGN KEY(feedId) REFERENCES feeds(_id)
);

deleteByFeedId:
DELETE FROM filterFeedRegistrations
WHERE feedId = ?;

deleteByFilterId:
DELETE FROM filterFeedRegistrations
WHERE filterId = ?;
