CREATE TABLE filters (
    _id INTEGER PRIMARY KEY AUTOINCREMENT,
    keyword TEXT,
    url TEXT,
    title TEXT,
    enabled INTEGER
);

getAll:
SELECT *
FROM filters
INNER JOIN filterFeedRegistrations
INNER JOIN feeds
WHERE filters._id = filterFeedRegistrations.filterId AND filterFeedRegistrations.feedId = feeds._id;

delete:
DELETE FROM filters
WHERE _id = ?;
